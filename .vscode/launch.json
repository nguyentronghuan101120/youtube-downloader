    "version": "0.2.0",
    "configurations": [
        {
            "name": "youtube-downloader",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "youtube-downloader (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "youtube-downloader (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Python: Execute Video Downloader",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/assets/excutable_for_app.py",
            "args": [
                "https://www.youtube.com/watch?v=H58vbez_m4E",
                "--format=audio",
                "--audio-format=mp3",
                // "--output-dir=DEFAULT_OUTPUT_DIR"
            ],
            "console": "integratedTerminal"
        },
    ]
}